# ESPHome Secrets Configuration
#
# Copy this file to secrets.yaml and fill in your values:
#   cp secrets.yaml.example secrets.yaml
#
# Generate an API encryption key with:
#   openssl rand -base64 32

wifi_ssid: "your-wifi-ssid"
wifi_password: "your-wifi-password"

api_encryption_key: "base64-encoded-32-byte-key"

ota_password: "your-ota-password"

web_server_username: "admin"
web_server_password: "your-web-password"